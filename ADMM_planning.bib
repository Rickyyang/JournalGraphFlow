	keywords = {Motion planning,automated vehicles,intelligent transportation systems,path planning},
	mendeley-groups = {Ref related work},
	number = {4},
	pages = {1135--1145},
	title = {{A Review of Motion Planning Techniques for Automated Vehicles}},
	volume = {17},
	year = {2016},
	bdsk-url-1 = {https://doi.org/10.1109/TITS.2015.2498841}}

