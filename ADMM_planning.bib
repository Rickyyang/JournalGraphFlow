@article{Sim2004,
	abstract = {Tremendous progress has been made recently in simultaneous localization and mapping of unknown environments. Using sensor and odometry data from an exploring mobile robot, it has become much easier to build high-quality globally consistent maps of many large, real-world environments. To date, however, relatively little attention has been paid to the controllers used to build these maps. Existing exploration strategies usually attempt to cover the largest amount of unknown space as quickly as possible. Few strategies exist for building the most reliable map possible, but the particular control strategy can have a substantial impact on the quality of the resulting map. In this paper, we devise a control algorithm for exploring unknown space that explicitly tries to build as large a map as possible while maintaining as accurate a map as possible. We make use of a parameterized class of spiral trajectory policies, choosing a new parameter setting at every time step to maximize the expected reward of the policy. We do this in the context of building a visual map of an unknown environment, and show that our strategy leads to a higher accuracy map faster than other candidate controllers, including any single choice in our policy class.},
	author = {Sim, R. and Dudek, G. and Roy, N.},
	doi = {10.1109/robot.2004.1308078},
	file = {:C$\backslash$:/Users/zy259/OneDrive/Work Files/Class 2018/proj 2018 summer/Online Control Policy Optimization for Minimizing.pdf:pdf},
	number = {April},
	pages = {1758--1763 Vol.2},
	title = {{Online control policy optimization for minimizing map uncertainty during exploration}},
	year = {2004},
	bdsk-url-1 = {https://doi.org/10.1109/robot.2004.1308078}}

